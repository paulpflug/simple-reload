{
  "name": "simple-reload",
  "description": "deletes a package from cache and reloads it - zero dependencies",
  "version": "0.0.2",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/simple-reload"
  },
  "engines": {
    "node": "*"
  },
  "main": "lib/index.js",
  "dependencies": {
  },
  "devDependencies": {
    "rimraf": "^2.3.1",
    "nodemon": "^1.3.7",
    "coffee-script": "^1.9.3"
  },
  "keywords": [
    "require",
    "cache",
    "reload"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "rimraf lib/ && mkdir lib",
    "prebuild": "npm run clean",
    "build": "coffee --compile --output lib/ src/*.coffee",
    "watch": "coffee --compile --watch --output lib/ src/*.coffee"
  }
}
